{
  "yaml.schemas": {
    "https://raw.githubusercontent.com/yannh/kubernetes-json-schema/master/v1.22.4-standalone-strict/all.json": [
      "/deployment.yaml",
      "/service.yaml",
      "/ingress.yaml"
    ],
    "http://json.schemastore.org/kustomization": ["/kustomization.yaml"],
    "https://gitlab.com/gitlab-org/gitlab/-/raw/master/app/assets/javascripts/editor/schema/ci.json": ["/.gitlab-ci.yml", "/project-*.yaml", "build-image.yaml"]
  },
  "snippets.ultisnips.pythonPrompt": false,
  "snippets": {
    "ultisnips.enable": false,
    "textmateSnippetsRoots": [
      "~/.config/nvim/snippets"
    ]
  }
}
